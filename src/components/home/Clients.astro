---
import { clients } from "../../constants/clients"
import ContainerSmall from "../../layouts/ContainerSmall.astro"
---

<section class="bg-clientsBackground w-full py-4">
	<ContainerSmall>
		<div class="grid grid-cols-1 lg:grid-cols-4 place-items-center">
			{
				clients.map((c) => (
					<img src={`/${c}`} alt="client logo" class="block" height="125" width="125"/>
				))
			}
		</div>
	</ContainerSmall>	
</section>